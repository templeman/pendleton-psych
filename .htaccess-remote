# Use PHP5 as default
	AddHandler application/x-httpd-php53 .php5 .php4 .php .php3 .php2 .phtml

	Options +FollowSymlinks
	RewriteEngine On
	RewriteBase /

# Add WWW
	rewritecond %{http_host} ^pendletonpsych.com [nc]
	rewriterule ^(.*)$ http://www.pendletonpsych.com/$1 [r=301,l]

